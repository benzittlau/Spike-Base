-if @versions
	%br/
	%table
		%tr
			User History
		%tr
			%td Date
			%td User
			%td Action
			%td Version
			%td View
		- @versions.each do |version|
			%tr
				%td 
					= l version.created_at, :format => :history_table
				%td
					= version.user
				%td
					= action_helper(version)
				%td
					= version.number
				%td
					= link_to "View", user_audit_path(:id => @user.id, :version => version.number )